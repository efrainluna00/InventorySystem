	<queryString><![CDATA[select (b.anio ||'-'||b.correlativo_mov) as llave,to_char(b.fecha_mov, 'DD/MM/YYYY HH24:MM:SS') as fecha_mov,d.cod_tipo_mov,d.descripcion as nombre_mov, 
       a.cod_recurso,c.nombre as nombre_recurso ,coalesce(c.marca,'') as marca,coalesce(c.modelo,'') as modelo,a.cantidad,e.cod_solicitud,activos.obt_nombre_usuario(e.cod_solicitante,4) as nombre_solicitante
	from inventario.inv_det_movimiento a
	inner join inventario.inv_movimiento b on (b.anio = a.anio and b.correlativo_mov = a.correlativo_mov)
	inner join inventario.inv_recurso c on (c.cod_recurso = a.cod_recurso)
	inner join inventario.inv_tipo_mov d on (d.cod_tipo_mov = b.cod_tipo_mov)
	left outer join activos.act_solicitud e on (e.tipo_solicitud = b.tipo_sol and e.cod_solicitud = b.cod_sol)
	
order by llave,c.nombre


]]></queryString>

