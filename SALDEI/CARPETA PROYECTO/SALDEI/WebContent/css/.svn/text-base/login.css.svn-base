* { padding: 0; margin: 0; }

body {
 font-family: Arial, Helvetica, sans-serif;
 font-size: 13px;
}
#wrapper { 
 margin: 0 auto;
 width: 944px;

}
#header {
 position:relative; 
 text-align: center;  
 font-size: 36px;
 color: #ffffff;
 width: 922px;
 padding: 10px;
 height: 100px;
 margin: 10px 0px 0px 0px;
 background: url(../images/login/azul2.gif);
}
#leftcolumn { 
 color: #333;
 background: #000000;
 margin-left: auto; margin-right: auto; 
 padding: 10px;
 height: 413px;
 width: 625px;
 float: left;
}
#rightcolumn { 
 float: right;
 color: #ffffff;
 background: url(../images/login/cel.gif);
 margin-left: auto; margin-right: auto; 
 padding: 10px;
 height: 413px;
 width: 275px;

}
#footer { 
 width: 922px;
 height: 60px;
 clear: both;
 color: #333;
 
 text-align: center; 
 background: url(../images/login/Plateado.gif);
 margin: 0px 0px 10px 0px;
 padding: 10px;
}

fieldset {
	border: 2px solid #439FD7;
	text-align: center;
	vertical-align: top;
}

