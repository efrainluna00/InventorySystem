package com.saldei.util.commons;
import com.saldei.util.commons.Base64;
public class Pruebas {
	public static void main(String[] args){
		String password="saldeixxxx123231";
		// codifica password
		String passwordEncriptado=Base64.encode(password);
		
		// para decodificarlo sale de la base y lo desencriptas 
		String passwordDesencriptado=Base64.decode(passwordEncriptado);
		
		System.out.println("Original: "+password);
		System.out.println("Encriptado: "+passwordEncriptado);
		System.out.println("Desencriptado: "+passwordDesencriptado);
		
		
	}
	
	
}
