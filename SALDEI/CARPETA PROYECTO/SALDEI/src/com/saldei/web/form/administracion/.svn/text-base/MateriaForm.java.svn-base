package com.saldei.web.form.administracion;

import org.apache.struts.action.ActionForm;

public class MateriaForm extends ActionForm {
	
	private static final long serialVersionUID = 1L;
	private String nombre;
	private String desc;
	private String estado;
	private String codigo;
	private String codigoHidden;
	private String uv;
	
	public void setUv(String uv){
		this.uv = uv;
	}
	public String getUv(){
		return uv;
	}
	public String getCodigoHidden() {
		return codigoHidden;
	}
	public void setCodigoHidden(String codigoHidden) {
		this.codigoHidden = codigoHidden;
	}
	public String getCodigo() {
		return codigo;
	}
	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	public String getEstado() {
		return estado;
	}
	public void setEstado(String estado) {
		this.estado = estado;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	

}
